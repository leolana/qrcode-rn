{
  "expo": {
    "name": "It Lab - QrCode",
    "description": "This is a poc using react-native",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/images/app-icon.png",
    "splash": {
      "image": "./public/images/launch.png",
      "resizeMode": "contain",
      "backgroundColor": "#473be7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
